<h1>Listing proceeds</h1>

<table>
  <thead>
    <tr>
      <th>Provider</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proceeds.each do |proceed| %>
      <tr>
        <td><%= proceed.provider %></td>
        <td><%= proceed.amount %></td>
        <td><%= link_to 'Show', proceed %></td>
        <td><%= link_to 'Edit', edit_proceed_path(proceed) %></td>
        <td><%= link_to 'Destroy', proceed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proceed', new_proceed_path %>
